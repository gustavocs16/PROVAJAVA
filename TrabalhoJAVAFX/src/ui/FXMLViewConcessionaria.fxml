<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="800.0" maxWidth="600.0" minHeight="487.0" minWidth="410.0" prefHeight="487.0" prefWidth="461.0" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerViewConcessionariaFXML">

    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
   <children>
   
       <TableView fx:id="table">
       <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
           <columns>
               <TableColumn fx:id="colID" maxWidth="40" minWidth="40" text="ID" />
               <TableColumn fx:id="colNome" text="Nome" />
               <TableColumn fx:id="colLocalidade" text="Localidade" />
           </columns>
       </TableView>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="btnDeletar" layoutX="71.0" layoutY="14.0" mnemonicParsing="false" onAction="#deleta" text="Deletar" />
            <Button fx:id="btnAtualiza" layoutY="14.0" mnemonicParsing="false" text="Atualizar" />
            <Button fx:id="btnSair" layoutX="133.0" layoutY="14.0" mnemonicParsing="false" onAction="#fechar" text="Fechar" />
         </children>
      </AnchorPane>
   </children>
</VBox>
